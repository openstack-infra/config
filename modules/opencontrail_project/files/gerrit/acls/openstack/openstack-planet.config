[access "refs/heads/*"]
        label-Code-Review = -2..+2 group opencontrail-planet-core
        label-Approved = +0..+1 group opencontrail-planet-core
        workInProgress = group opencontrail-planet-core
[project]
        state = active
[receive]
        requireChangeId = true
        requireContributorAgreement = true
[submit]
        mergeContent = true
