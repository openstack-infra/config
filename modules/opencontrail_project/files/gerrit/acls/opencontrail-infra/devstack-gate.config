[access "refs/heads/*"]
        label-Code-Review = -2..+2 group devstack-gate-core
        label-Approved = +0..+1 group devstack-gate-core
        workInProgress = group devstack-gate-core
[project]
        state = active
[receive]
        requireChangeId = true
        requireContributorAgreement = true
[submit]
        mergeContent = true
