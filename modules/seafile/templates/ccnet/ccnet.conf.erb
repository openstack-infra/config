[General]
USER_NAME = <%= seafile_user_name %>
# ID is generated from
# https://github.com/haiwen/seafile/blob/master/gui/common/ccnet-init.c#L125
ID = <%= seafile_id %>
NAME = <%= seafile_instance_name %>
# seafile_url includes port number, defaults to 8000
SERVICE_URL = <%= seafile_url %>

[Network]
# default is 10001
PORT = <%= seafile_network_port %>

[Client]
#default is 13418
PORT = <%= seafile_client_port %>

[Database]
# default engine is mysql on port 3306
ENGINE = <%= seafile_db_engine %>
HOST = <%= seafile_db_host %>
PORT = <%= seafile_db_port %>
USER = <%= seafile_db_user %>
PASSWD = <%= seafile_db_password %>
DB = <%= seafile_ccnet_db %>
