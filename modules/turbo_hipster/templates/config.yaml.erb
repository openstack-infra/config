zuul_server:
  gerrit_site: <%= @gerrit_site %>
  git_origin: <%= @git_origin %>
  gearman_host: <%= @gearman_server %>
  gearman_port: <%= @gearman_port %>

debug_log: /var/log/turbo-hipster/debug.log
jobs_working_dir: /var/lib/turbo-hipster/jobs
git_working_dir: /var/lib/turbo-hipster/git
pip_download_cache: /var/cache/pip
baseline_command: "baseline.sh %s"
flavor: rackspace_standard_2cpu_2gb

publish_logs:
  type: swift
  authurl: https://identity.api.rackspacecloud.com/v2.0/
  tenant: " "
  user: <%= @rs_cloud_user %>
  password: <%= @rs_cloud_pass %>
  container: syd_turbo_hipster_results_v2
  region: SYD
  prepend_url: http://www.rcbops.com/turbo_hipster/results/

conf_d: /etc/turbo-hipster/conf.d/
