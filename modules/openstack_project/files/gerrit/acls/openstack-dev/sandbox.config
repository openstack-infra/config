[access "refs/*"]
        owner = group Administrators
[receive]
        requireChangeId = true
        requireContributorAgreement = true
[submit]
        mergeContent = true
[access "refs/heads/*"]
        label-Code-Review = -2..+2 group openstack-cla
        label-Approved = +0..+1 group openstack-cla
[access "refs/heads/milestone-proposed"]
        label-Code-Review = -2..+2 group openstack-cla
        label-Approved = +0..+1 group openstack-cla
