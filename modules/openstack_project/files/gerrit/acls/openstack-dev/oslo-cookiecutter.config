[access "refs/heads/*"]
        label-Workflow = -1..+1 group oslo-core
        label-Code-Review = -2..+2 group oslo-core
[access "refs/tags/*"]
        create = group oslo-core
        pushTag = group oslo-core
[project]
        state = active
[receive]
        requireChangeId = true
        requireContributorAgreement = true
[submit]
        mergeContent = true
