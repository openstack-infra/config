[access "refs/heads/*"]
        label-Code-Review = -2..+2 group nova-drivers
        label-Approved = +0..+1 group nova-drivers
        workInProgress = group nova-drivers
[access "refs/heads/milestone-proposed"]
        label-Code-Review = -2..+2 group nova-milestone
        label-Approved = +0..+1 group nova-milestone
[project]
        state = active
[receive]
        requireChangeId = true
        requireContributorAgreement = true
[submit]
        mergeContent = true
