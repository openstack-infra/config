[access "refs/*"]
   owner = group Administrators
[receive]
   requireChangeId = true
   requireContributorAgreement = true
[submit]
   mergeContent = true
[access "refs/heads/*"]
   label-Code-Review = -2..+2 group os-apply-config-core
   label-Approved = +0..+1 group os-apply-config-core
   workInProgress = group os-apply-config-core
[access "refs/tags/*"]
   create = group os-apply-config-core
   pushTag = group os-apply-config-core
[project]
   state = active
