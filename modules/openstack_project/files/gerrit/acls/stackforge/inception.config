[access "refs/heads/*"]
        label-Code-Review = -2..+2 group inception-core
        label-Workflow = -1..+1 group inception-core
        abandon = group inception-core
[access "refs/tags/*"]
        create = group inception-core
        pushSignedTag = group inception-core
[receive]
        requireChangeId = true
        requireContributorAgreement = true
[submit]
        mergeContent = true
