[access "refs/heads/*"]
    label-Code-Review = -2..+2 group rubick-core
    label-Workflow = -1..+1 group rubick-core
    abandon = group rubick-core
[access "refs/tags/*"]
    create = group rubick-ptl
    pushSignedTag = group rubick-ptl
[receive]
    requrieChangeId = true
    requireContributorAgreement = true
[submit]
    mergeContent = true
