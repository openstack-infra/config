[access "refs/heads/*"]
        label-Code-Review = -2..+2 group openstack-smartos-core
        label-Approved = +0..+1 group openstack-smartos-core
        workInProgress = group openstack-smartos-core
[access "refs/tags/*"]
        create = group openstack-smartos-core
