[access "refs/heads/*"]
        label-Code-Review = -2..+2 group python-ipmi-core
        label-Workflow = -1..+1 group python-ipmi-core
[access "refs/tags/*"]
        create = group python-ipmi-core
        pushTag = group python-ipmi-core
[receive]
        requireChangeId = true
        requireContributorAgreement = true
[submit]
        mergeContent = true
