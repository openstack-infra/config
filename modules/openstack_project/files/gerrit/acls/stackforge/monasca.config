[access "refs/heads/*"]
        label-Code-Review = -2..+2 group monasca-core
        label-Workflow = -1..+1 group monasca-core
        abandon = group monasca-core
[access "refs/tags/*"]
        pushSignedTag = group monasca-ptl
[receive]
        requireChangeId = true
        requireContributorAgreement = true
[submit]
        mergeContent = true
