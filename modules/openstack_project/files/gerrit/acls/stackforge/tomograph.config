[access "refs/heads/*"]
abandon = group tomograph-core
label-Code-Review = -2..+2 group tomograph-core
label-Workflow = -1..+1 group tomograph-core

[access "refs/tags/*"]
pushSignedTag = group tomograph-core

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
