[access "refs/heads/*"]
        label-Code-Review = -2..+2 group surveil-core
        label-Workflow = -1..+1 group surveil-core
        abandon = group surveil-core
[access "refs/tags/*"]
        pushSignedTag = group surveil-ptl
[receive]
        requireChangeId = true
        requireContributorAgreement = true
[submit]
        mergeContent = true
