[access "refs/heads/*"]
        label-Code-Review = -2..+2 group dotnet-sdk-core
        label-Workflow = -1..+1 group dotnet-sdk-core
        abandon = group dotnet-sdk-core
[access "refs/tags/*"]
        create = group dotnet-sdk-ptl
        pushSignedTag = group dotnet-sdk-ptl
[receive]
        requireChangeId = true
        requireContributorAgreement = true
[submit]
        mergeContent = true