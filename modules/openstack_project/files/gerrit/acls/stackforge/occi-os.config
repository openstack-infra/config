[access "refs/heads/*"]
abandon = group occi-core
label-Code-Review = -2..+2 group occi-core
label-Workflow = -1..+1 group occi-core

[access "refs/tags/*"]
create = group occi-core
pushSignedTag = group occi-core

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
