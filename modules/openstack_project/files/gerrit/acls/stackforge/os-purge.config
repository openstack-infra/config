[access "refs/heads/*"]
        label-Code-Review = -2..+2 group os-purge-core
        label-Approved = +0..+1 group os-purge-core
        workInProgress = group os-purge-core
[access "refs/tags/*"]
        create = group os-purge-core
        pushTag = group os-purge-core
[receive]
        requireChangeId = true
        requireContributorAgreement = false
[submit]
        mergeContent = true
