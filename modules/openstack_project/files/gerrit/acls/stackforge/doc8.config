[access "refs/heads/*"]
        label-Code-Review = -2..+2 group doc8-core
        label-Workflow = -1..+1 group doc8-core
        abandon = group doc8-core
[access "refs/tags/*"]
        pushSignedTag = group doc8-ptl
[project]
        state = active
[receive]
        requireChangeId = true
        requireContributorAgreement = false
[submit]
        mergeContent = true
