[access "refs/*"]
        pushtag = group openstack-ci-admins
        create = group openstack-ci-admins
[access "refs/heads/*"]
        label-code-review = -2..+2 group openstack-ci-admins
        label-approved = +0..+1 group openstack-ci-admins
[project]
        state = active
[receive]
        requirechangeid = true
[submit]
        mergecontent = true
