[access "refs/heads/*"]
label-Code-Review = -2..+2 group infra-core
label-Workflow = -1..+1 group infra-core

[access "refs/meta/config"]
label-Code-Review = -2..+2 group infra-core
label-Verified = -2..+2 group infra-core
label-Workflow = -1..+1 group infra-core
submit = group infra-core

[receive]
requireChangeId = true
requireShortMessage = true

[submit]
mergeContent = true
