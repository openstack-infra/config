[access "refs/heads/*"]
        label-Workflow = -1..+1 group openstack-ci-core
        label-Code-Review = -2..+2 group openstack-ci-core
[access "refs/tags/*"]
        create = group openstack-ci-core
        pushTag = group openstack-ci-core
[receive]
        requireChangeId = true
[submit]
        mergeContent = true
