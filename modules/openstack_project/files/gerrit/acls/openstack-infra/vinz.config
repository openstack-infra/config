[access "refs/heads/*"]
        label-Code-Review = -2..+2 group vinz-core
        label-Workflow = -1..+1 group vinz-core
        abandon = group vinz-core
[access "refs/tags/*"]
        pushSignedTag = group vinz-ptl
[receive]
        requireChangeId = true
        requireContributorAgreement = true
[submit]
        mergeContent = true
