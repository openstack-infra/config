[access "refs/*"]
        pushTag = group openstack-ci-admins
        create = group openstack-ci-admins
[access "refs/heads/*"]
        label-Code-Review = -2..+2 group openstack-ci-admins
        label-Workflow = -1..+1 group openstack-ci-admins
[project]
        state = active
[receive]
        requireChangeId = true
[submit]
        mergeContent = true
