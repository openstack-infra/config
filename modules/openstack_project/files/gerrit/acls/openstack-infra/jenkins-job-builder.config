[access "refs/heads/*"]
        label-Approved = +0..+1 group jenkins-job-builder-core
        label-Code-Review = -2..+2 group jenkins-job-builder-core
[access "refs/tags/*"]
        create = group jenkins-job-builder-release-managers
        pushTag = group jenkins-job-builder-release-managers
[project]
        state = active
[receive]
        requireChangeId = true
[submit]
        mergeContent = true
