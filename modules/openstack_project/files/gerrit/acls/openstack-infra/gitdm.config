[access "refs/heads/*"]
        label-Workflow = -1..+1 group stats-core
        abandon = group stats-core
        label-Code-Review = -2..+2 group stats-core
[access "refs/tags/*"]
        create = group stats-core
        pushSignedTag = group stats-core
[project]
        state = active
[receive]
        requireChangeId = true
[submit]
        mergeContent = true
