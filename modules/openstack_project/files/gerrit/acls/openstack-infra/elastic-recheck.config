[access "refs/*"]
        create = group elastic-recheck-ptl
[access "refs/heads/*"]
        label-Approved = +0..+1 group elastic-core
        label-Code-Review = -2..+2 group elastic-core
[access "refs/tags/*"]
        create = group elastic-recheck-ptl
        pushTag = group elastic-recheck-ptl
[project]
        state = active
[receive]
        requireChangeId = true
        requireContributorAgreement = true
[submit]
        mergeContent = true
