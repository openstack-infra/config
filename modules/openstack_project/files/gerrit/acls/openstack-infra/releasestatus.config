[access "refs/heads/*"]
        label-Code-Review = -2..+2 group openstack-releasestatus
        label-Approved = +0..+1 group openstack-releasestatus
[access "refs/tags/*"]
        create = group openstack-releasestatus
        pushTag = group openstack-releasestatus
[project]
        state = active
[receive]
        requireChangeId = true
[submit]
        mergeContent = true
        action = cherry pick
