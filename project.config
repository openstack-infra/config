[access "refs/*"]
	owner = group Administrators
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group openstack-ci-admins
	label-Approved = +0..+1 group openstack-ci-admins
[receive]
	requireChangeId = false
	requireShortMessage = true
[submit]
	mergeContent = true
	action = cherry pick
[project]
	state = active
